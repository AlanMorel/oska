{
    "name": "oska",
    "version": "1.0.0",
    "author": "Alan Morel",
    "license": "MIT",
    "description": "A Discord bot written using Node in TypeScript",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlanMorel/oska.git"
    },
    "scripts": {
        "dev": "nodemon --config ./nodemon.config.json",
        "lint": "eslint ./src --fix",
        "start": "node --loader ts-paths-esm-loader --experimental-specifier-resolution=node ./src/Bot.ts",
        "ts:check": "tsc --noEmit"
    },
    "dependencies": {
        "@types/node": "^18.15.11",
        "cross-env": "^7.0.3",
        "discord.js": "^14.9.0",
        "dotenv": "^16.0.3",
        "picocolors": "^1.0.0",
        "ts-node": "^10.9.1",
        "ts-paths-esm-loader": "^1.4.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.5.1",
        "@commitlint/config-conventional": "^17.4.4",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-security": "^1.7.1",
        "husky": "^8.0.3",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7"
    }
}
